.clickBlock5, .clickBlock5:hover{background-image: url(http://static.365rili.com/wannianlibaidu/BD_images/bg2.jpg);	background-position: -436px -235px; width: 50px; height: 51px; }
.clickBlock6, .clickBlock6:hover{background-image: url(http://static.365rili.com/wannianlibaidu/BD_images/bg2.jpg);	background-position: -436px -297px; width: 50px; height: 43px;}

/*某些日期的间距和高度[受absolute影响]*/
.number6{margin-top: 8px;}
.number4{margin-top: 25px;}
.block4:hover{background: url(http://static.365rili.com/wannianlibaidu/BD_images/bg2.jpg) no-repeat top left; background-position: -224px -276px; width: 50px; height: 64px;}
.block5:hover{background: url(http://static.365rili.com/wannianlibaidu/BD_images/bg2.jpg) no-repeat top left; background-position: -489px -235px; width: 50px; height: 51px;}
.block6:hover{background: url(http://static.365rili.com/wannianlibaidu/BD_images/bg2.jpg) no-repeat top left; background-position: -489px -297px; width: 50px; height: 43px;}
.block_content4{height: 64px;}
.block_content6{height: 43px;}
/*农历距上的间距[受absolute影响]*/
.chinaday6{margin-top: 28px;}
.chinaday4{margin-top: 46px;}

/*日程icon*/
.workDot{position: absolute; margin-left: 41px;	height: 9px; width: 9px; overflow: hidden;}
.workDot6{margin-top: 33px;}
.workDot5{margin-top: 42px;}
.workDot4{margin-top: 55px;}

#cal_365riliLogo{float: left; background-image: url(http://static.365rili.com/wannianlibaidu/BD_images/365logo.png);	cursor: pointer; height: 23px;	width: 24px;}
#help-text{height: 13px; width: 153px; float: left; color: #ffffff; margin-left: 22px; font-size: 13px;	line-height: 17px;	text-decoration: underline;}
#cal_downright{	width: 109px; height: 100%; float: left;}
#workBoard{	height: 58px; 	width: 100%; 	background-color: #fbfbe0;	border-top: 1px dashed #ccc;	font-family:Microsoft YaHei;}
.morework{	line-height:18px;	color: #aaa;	font-size: 12px;	margin-left: 16px;	height: 18px;	cursor: pointer;	width: 90px;	overflow: hidden;}
.work_item{	height: 18px;	font-size: 18px;	line-height:18px;	width: 105px;	overflow: hidden;}
.work_item_content{	font-size: 14px;	color: #666261;}
#select-time{display: none;}
.popDivItemRight{	float: right;	font-size: 17px;	color: red;	font-family: arial;}
.workitem{	clear: both;	font-size: 13px;	color: #626262;	margin-bottom: 2px;}
.tastWorkDot{	float: left;	background-image: url(http://static.365rili.com/wannianlibaidu/BD_images/bg2.jpg);	background-position: 0 -997px; width: 6px; height: 7px;	width: 6px;	height: 7px;	margin-top: 5px;}
.taskWorkTime{	float: left;	color: #cccccc;	margin-left: 5px;}
.cover{	background: none repeat scroll 0 0 #000000;	position: absolute;	left: 0;top: 0;	opacity: 0.6;	filter:alpha(opacity=60);	width: 100%;	z-index: 1999;	height:110%;}
.loading{	height:100px;	width:100px;	position:absolute;}
.fixed-tip{	height:18px;	padding-left:5px;	padding-right:5px;	background-color:#C6322A;	color:white;	position:fixed;	top:1px;	left:240px;}
.popup-menu{	position:fixed;	width:240px;	border:1px solid #CCCCCC;	background:#FFFFFF;	z-index:1999;}
.popup-menu a,.popup-menu a:hover{	display:block;	width:228px;	height:24px;	padding-top:3px;	padding-bottom:3px;	padding-left:12px;	float:left;	text-decoration:none;	color:#666666;	font-size:14px;	font-weight: normal;	text-align: left;}
.popup-menu a:hover{	background:#CCCCCC;}
