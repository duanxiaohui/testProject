		<script type='text/javascript'>
			$(function(){
				var mainWidth = window.screen.width;
				//$("#middle").width(mainWidth);
			});
			$("#downphone_btn").click(function(){
				var sUserAgent = navigator.userAgent.toLowerCase();
				var bIsIpad = sUserAgent.match(/ipad/i) == "ipad"; 
 				var bIsIphoneOs = sUserAgent.match(/iphone os/i) == "iphone os";
				var bIsAndroid = sUserAgent.match(/android/i) == "android";
				if(bIsIphoneOs){
					$(this).attr("herf","http://www.365rili.com/iosexchange/wapapp");
					if(confirm("确认要下载365日历手机苹果客户端")){
						return;
					}else{
						return false; 
					}
				}
				if(bIsAndroid){
					$(this).attr("herf","d2.365rili.com/dl/android/exchange/365rili_wapapp.apk");
					if(confirm("确认要下载365日历手机安卓客户端")){
						return;
					}else{
						return false; 
					}
				}
				
			});
