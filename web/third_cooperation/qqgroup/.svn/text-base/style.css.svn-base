	border-bottom: solid 1px rgba(221,221,221,.5);
}
.tschedule:nth-child(2):after{
	background: url(/third_cooperation/qqgroup/images/icon_ts1.png) no-repeat 0;
	background-size: 8px 8px;
}
.tschedule:nth-child(3):after{
	background: url(/third_cooperation/qqgroup/images/icon_ts2.png) no-repeat 0;
	background-size: 8px 8px;
}
