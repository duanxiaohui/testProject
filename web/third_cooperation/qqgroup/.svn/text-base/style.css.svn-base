.checkBox{
	width: 52.5px;
	height: 32.5px;
	overflow: hidden;
	float: right;
}
.ios7CBox{
    margin:0;
	padding:0;
	border:none;
	width:105px;
	height:65px;
	position:relative;
	color:#fff;
	background:none;
	-webkit-appearance: none;
	-moz-appearance: none;
	-ms-appearance: none;
	-o-appearance: none;
	appearance: none;
	-webkit-transform: scale(0.5);
	-webkit-transform-origin:0 0;
}
.ios7CBox:before,
.ios7CBox:after {
    position: absolute;
    left: 0;
    top: 0;
    content: '';
}
.ios7CBox:before {
    transition: all 0.4s;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    border-radius: 33px;
    background: #fff;
    border: 3px solid #E5E5E5;
}
.ios7CBox:after {
    transition: all 0.2s;
    border-radius: 33px;
    top: 4px;
    left: 4px;
    width: 56px;
    height: 56px;
    background: #fff;
    box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.3), 0px 0px 2px rgba(0, 0, 0, 0.3);
    -webkit-animation: ios7CBox_after 0.4s;
    -webkit-animation-fill-mode: both;
    -moz-animation: ios7CBox_after 0.4s;
    -moz-animation-fill-mode: both;
}
.ios7CBox:hover:after {
    box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.4), 0px 0px 2px rgba(0, 0, 0, 0.3);
}
.ios7CBox:focus:before {
    border: 3px solid #C6C6C6;
}
.ios7CBox:focus {
    outline: none;
}
.ios7CBox:checked:before {
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    border-style: solid;
    border-color: #31b1f3;
    border-width: 32px 32px 32px 64px;
}
.ios7CBox:checked:after {
    -webkit-animation: ios7CBox_after_checked 0.4s;
    -webkit-animation-fill-mode: both;
    -moz-animation: ios7CBox_after_checked 0.4s;
    -moz-animation-fill-mode: both;

    left: 45px;
}
.ios7CBox:active:after {
    width: 64px;
}
.ios7CBox:active:checked:after {
    width: 64px;
    margin-left: -8px;
}
@-webkit-keyframes ios7CBox_after {
    0% {
        left: 45px;
    }
    
    70% {
        left: 1px;
    }
    
    100% {
        left: 4px;
    }
}
@-moz-keyframes ios7CBox_after {
    0% {
        left: 45px;
    }
    
    70% {
        left: 1px;
    }
    
    100% {
        left: 4px;
    }
}
@-webkit-keyframes ios7CBox_after_checked {
    0% {
        left: 4px;
    }
    
    70% {
        left: 47px;
    }
    
    100% {
        left: 45px;
    }
}
@-moz-keyframes ios7CBox_after_checked {
    0% {
        left: 4px;
    }
    
    70% {
        left: 47px;
    }
    
    100% {
        left: 45px;
    }
}
.ios7CBox:disabled:before, .ios7CBox:disabled:after {
    background: #f4f4f4;
}
.ios7CBox:disabled:active:after {
    width: 56px;
}
.ios7CBox:disabled:active:checked:after {
    width: 56px;
    margin-left: 0px;
}
.ios7CBox:disabled:active:after {
    width: 56px;
}
.ios7CBox:disabled:active:checked:after {
    width: 56px;
    margin-left: 0px;
}
.ios7CBox:disabled:hover:after {
    box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.3), 0px 0px 2px rgba(0, 0, 0, 0.3);
}
.ios7CBox:disabled:checked:before {
    border-color: #ddd;
}


/*------- 动画基础样式 ------*/
.animated {
    -webkit-animation-duration: 1s;
    -webkit-animation-fill-mode: both;
}

.animated.infinite {
    -webkit-animation-iteration-count: infinite;
}

.animated.hinge {
    -webkit-animation-duration: 4s;
}

.animated.stop{
	-webkit-animation-play-state:paused;
	-webkit-animation-name: none;
}

@-webkit-keyframes fadeInDown {
    0% {
        opacity: 0;
        -webkit-transform: translate3d(0, -30px, 0);
    }
    100% {
        opacity: 1;
        -webkit-transform: none;
    }
}

.fadeInDown {
    -webkit-animation-name: fadeInDown;
}

@-webkit-keyframes fadeInUp {
    0% {
        opacity: 0;
        -webkit-transform: translate3d(0, 30px, 0);
    }
    100% {
        opacity: 1;
        -webkit-transform: none;
    }
}

.fadeInUp {
    -webkit-animation-name: fadeInUp;
}


@-webkit-keyframes flip {
    0% {
        -webkit-transform: perspective(400px) translate3d(0, 0, -500px) rotate3d(0, 1, 0, -220deg);
        -webkit-animation-timing-function: ease-in;
        opacity: 1
    }
    10% {
        -webkit-transform: perspective(400px) translate3d(0, 0, -400px) rotate3d(0, 1, 0, -180deg);
        -webkit-animation-timing-function: ease-in;
        opacity: 1
    }
    20% {
        -webkit-transform: perspective(400px) translate3d(0, 0, -200px) rotate3d(0, 1, 0, -90deg);
        -webkit-animation-timing-function: ease-in;
        opacity: 1
    }
    25% {
        -webkit-transform: perspective(400px) translate3d(0, 0, 80px) rotate3d(0, 1, 0, 0deg);
        -webkit-animation-timing-function: ease-in;
        opacity: 1
    }
    30%{
		-webkit-transform: perspective(400px);
        -webkit-animation-timing-function: ease-in;
        opacity: 1
    }
    33%{
    	-webkit-transform:scale3d(.95, .95, .95);
        -webkit-animation-timing-function: ease-in;
        opacity: 1
    }
    36%{
    	-webkit-transform:scale3d(1, 1, 1);
        -webkit-animation-timing-function: ease-in;
        opacity: 1
    }
    100%{
    	opacity: 1;
    }
}

.animated.flip {
    -webkit-animation-duration: 2s;
    -webkit-backface-visibility: visible;
    -webkit-animation-name: flip;
}


@-webkit-keyframes swing {
    20% {
        -webkit-transform: rotate3d(0, 0, 1, 15deg);
    }
    40% {
        -webkit-transform: rotate3d(0, 0, 1, -10deg);
    }
    60% {
        -webkit-transform: rotate3d(0, 0, 1, 5deg);
    }
    80% {
        -webkit-transform: rotate3d(0, 0, 1, -5deg);
    }
    100% {
        -webkit-transform: rotate3d(0, 0, 1, 0deg);
    }
}
.swing {
    -webkit-transform-origin: top center;
    -webkit-animation-name: swing;
}
@-webkit-keyframes fadeOut {
    0% {
        opacity: 1
    }
    50%, 100% {
        opacity: 0;
        -webkit-transform: perspective(400px) translate3d(0, 0, 80px)
    }
}

.fadeOut {
    -webkit-animation-name: fadeOut;
}

@-webkit-keyframes qqshake {
    0%, 100% {
        -webkit-transform: translateY(0);
    }
    20%,
    60%{
        -webkit-transform: translateY(-19px);
        opacity: 0.5;
    }
    40%,
    80% {
        -webkit-transform: translateY(19px);
        opacity: 0.5;
    }
}

.qqshake {
    -webkit-animation-duration: .3s;
    -webkit-animation-name: qqshake;
}