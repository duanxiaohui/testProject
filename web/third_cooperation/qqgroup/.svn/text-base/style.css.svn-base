	width: 17.5px;
}
.tschedule a .icon{
	width: 56px;
	height: 56px;
}
.finished a{
	background: none;
}
.finished .msk{
	display: none;
}
.unfinished .icon{
	display: none;
}
.tschedule:nth-child(2) a{
	border: solid 2px #ffd5ab;
}
.tschedule:nth-child(3) a{
	border: solid 2px #b0e0df;
}
.tschedule:nth-child(4) a{
	border: solid 2px #fea08f;
}
.tschedule:last-child{
