	background: url(/third_cooperation/qqgroup/images/icon_ts3.png) no-repeat 0;
	background-size: 8px 8px;
}
.tschedule:after{
	position: absolute;
	content: ' ';
	width: 8px;
	height: 8px;
	top: 3px;
	left: -17px;
}
.tschedule a{
	background: url(/third_cooperation/qqgroup/images/icon_msk.png) no-repeat 0;
	background-size: 56px 56px;
	float: right;
	width: 56px;
	height: 56px;
	margin-right: 10px;
	border-radius: 100%;
	margin-top: -5px;
