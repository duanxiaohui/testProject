}
.tip:after{
	border-bottom: solid 1px #ddd;
	content: ' ';
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	-webkit-transform: scaleY(.5);
	-webkit-transform-origin:0 0;
}
.reTip{
	background-image: url(/third_cooperation/qqgroup/images/icon_re.png);
	background-size: 15px 14.5px;
}
.tipState{
	font-size: 13px;
	color:#a1aaaf;
	float: right;
	position: relative;
	padding-right: 15px;
}
.tipState:after{
	position: absolute;
	content: ' ';
	width: 6px;
	height: 6px;
	border-top: solid 1px #a1aaaf;
	border-left: solid 1px #a1aaaf;
	right: 0;
	top: 6px;
	-webkit-transform: rotate(-135deg);
	-webkit-transform-origin: 2px 2px;
	-webkit-transition: transform .3s;
}
.show .tipState:after{
	-webkit-transform: rotate(45deg);
	/*top: 6px;*/
}

.timePoint{
	width: 280px;
	overflow: hidden;
	height: 50px;
	-webkit-transition: all .3s;
}
.repeatSet div{
	width: 280px;
}
.repeatSet .line{
	width: 100%;
}
.repeatSet{
	overflow: hidden;
	height: 0;
	-webkit-transition: all .3s;
}
.show .timePoint, .show .repeatSet{
	height: 116px;
}
.timePoint span, .repeatSet span{
	margin-top: 14px;
	float: left;
	border: 1px solid #E5E5E5;
	border-radius: 3px;
	line-height: 34px;
	height: 34px;
	overflow: hidden;
	text-align: center;
	background-color: #fff;
	margin-right: 8px;
	width:62px;
	font-size: 13px;
}
.repeatSet span{
	width: 83px;
}
.ch:nth-child(4n+4) {
    margin-right: 0;
}
