body { background: #fff; color: #444; font: 12px / 1.5em "Microsoft YaHei", 微软雅黑,SimSun,arial; margin: 0; padding: 0; }
div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, textarea, blockquote, p { margin: 0; padding: 0; }
ul { list-style: none; }
li { list-style-type: none; }
img { border: 0; vertical-align: top; }
select, input { font-size: 12px; }
address, cite, code, em, th { font-style: normal; font-weight: normal; }
table { border-collapse: collapse; border-spacing: 0; table-layout: fixed; }
.e_clear:after { clear: both; content: "."; display: block; height: 0; visibility: hidden; }
.e_clear { display: inline-block; }
.e_clear { display: block; }
textarea { background: #fff; border: 1px solid #d2d9dc; border-radius: 2px; box-shadow: 0 0 0 #d2d9dc; -moz-transition: .3s ease-in-out; transition-property: border box-shadow; -webkit-transition: .3s ease-in-out; }
textarea:focus, input[type="text"]:focus { /*border: 1px solid #b7d4ea;*/ box-shadow: 0 0 5px #b7d4ea; }
a { color: #208de9; cursor: pointer; text-decoration: none; }
a:hover { color: #fff; }
.v_align { vertical-align: middle;}
.none { display: none; }
/*jq-ui*/
.ui-corner-all-16, .ui-corner-top-16, .ui-corner-left-16, .ui-corner-tl-16 { -moz-border-radius-topleft: 16px; -webkit-border-top-left-radius: 16px; -khtml-border-top-left-radius: 16px; border-top-left-radius: 16px; }
.ui-corner-all-16, .ui-corner-top-16, .ui-corner-right-16, .ui-corner-tr-16 { -moz-border-radius-topright: 16px; -webkit-border-top-right-radius: 16px; -khtml-border-top-right-radius: 16px; border-top-right-radius: 16px; }
.ui-corner-all-16, .ui-corner-bottom-16, .ui-corner-left-16, .ui-corner-bl-16 { -moz-border-radius-bottomleft: 16px; -webkit-border-bottom-left-radius: 16px; -khtml-border-bottom-left-radius: 16px; border-bottom-left-radius: 16px; }
.ui-corner-all-16, .ui-corner-bottom-16, .ui-corner-right-16, .ui-corner-br-16 { -moz-border-radius-bottomright: 16px; -webkit-border-bottom-right-radius: 16px; -khtml-border-bottom-right-radius: 16px; border-bottom-right-radius: 16px; }
.ui-shadow { box-shadow: 3px 3px 3px #d7d8d9; -moz-box-shadow: 3px 3px 3px #d7d8d9; -webkit-box-shadow: 3px 3px 3px #d7d8d9; }

.head{height:56px; width:100%; background:#77d7e2 url(../images/logo.png) no-repeat 15px center; -webkit-background-size:134px 34px;}
.coco,.todo,.wnl{position:relative;}
.coco{padding:10px 0 36px 0; min-height:380px;}
.coco p{font-size:29px; color:#070707; padding-left:15px; line-height:40px;}
.coco .coco_content{height:185px; margin:10% 5% 5% 5%; background:url(../images/coco_content.jpg) no-repeat right; -webkit-background-size:215px 185px;}
.coco_btn{display:block; width:229px; height:63px; margin:10px auto; font-size:25px; line-height:60px; text-align:center; color:#fff; background:#f60; -moz-border-radius: 5px; -webkit-border-radius: 5px; -khtml-border-radius: 5px; border-radius: 5px;}
.calendar_name1 h3,.calendar_name2 h3{font-size:16px; color:#070707; line-height:18px; font-weight:500;}
.calendar_name1 p,.calendar_name2 p{font-size:12px; line-height:14px; padding-left:0;}
.calendar_name1{margin:1% 0 0 15%;}
.calendar_name2{margin:15% 0 0 0;}
.coco_bottom{height:36px; background:#77d7e2 url(../images/coco_bottom.jpg) no-repeat 15% center; width:100%; -webkit-background-size:290px 25px; position:absolute; bottom:0;}

.todo{padding:0 0 0 5%; }
.todo h2,.wnl h2{width:210px; height:45px; border-bottom:2px solid #cfcfcf; line-height:45px; font-size:32px; color:#070707;}
.todo p,.wnl p{width:210px; line-height:180%; font-size:12px; color:#7b7b7b;}
.todo_btn,.wnl_btn{position:absolute; top:45px; right:0; width:90px; height:65px; text-align:center; line-height:65px; color:#fff; font-size:25px; background:#f60;}
.todo_box{height:240px;background:#fff url(../images/todo_pic.jpg) no-repeat center center; -webkit-background-size:241px 222px;}

.wnl{padding:0 0 0 5%;}
.wnl_box{height:240px; background:#fff url(../images/wnl_pic.jpg) no-repeat center center; -webkit-background-size:225px 204px;}

.weixin_layer{display:none; width:100%; height:100%; position:absolute; top:0; left:0; background:#fff url(../images/bg.gif) no-repeat right top; -webkit-background-size:320px 460px;}
.weixin_wap_layer{display:none; width:100%; height:100%; position:absolute; top:0; left:0; background:rgba(0,0,0,0.8) url(../images/arrow.png) no-repeat right 5px; -webkit-background-size:84px 70px; z-index:3;
text-align:center; color:#fff; font-size:24px; line-height:180%;  padding-top:60px;
}
.weixin_box{background:#fff url(../images/weixin_layer.jpg) no-repeat center 80px; -webkit-background-size:90px 35px;}
.weixin_box p{padding-top:125px; width:260px; line-height:180%; font-size:24px; color:#000; margin:0 auto; text-align:center;}
.weixin_box p span.red{color:#f00;}

@media (device-height:480px) and (-webkit-min-device-pixel-ratio:2){
	.coco{padding:0 0 46px 0; min-height:270px;}
	.coco p{font-size:20px; color:#070707; padding-left:10px; line-height:150%; font-weight:600;}
	.coco .coco_content{height:165px; margin:1% 5% 1% 5%; background:url(../images/coco_content.jpg) no-repeat right; -webkit-background-size:165px 145px;}
	.calendar_name1{padding:8% 0 0 0%;}
	.calendar_name2{margin:5% 0 0 0;}
	.calendar_name1 h3,.calendar_name2 h3{font-size:16px; color:#070707; line-height:16px; font-weight:500;}
	.calendar_name1 p,.calendar_name2 p{font-size:12px; line-height:14px; padding-left:0;}
	.coco_btn{display:block; width:229px; height:43px; margin:0 auto; font-size:25px; line-height:38px; text-align:center; color:#fff; background:#f60;}
}
@media (device-height:568px) and (-webkit-min-device-pixel-ratio:2){
	.coco{ min-height:358px;}
	.coco p{font-size:20px; color:#070707; padding-left:10px; line-height:150%; font-weight:600; }
	.coco .coco_content{height:165px; margin:2% 5% 1% 5%; background:url(../images/coco_content.jpg) no-repeat right; -webkit-background-size:165px 145px;}
	.calendar_name1{padding:13% 0 0 0%;}
	.calendar_name2{margin:5% 0 0 0;}
	.calendar_name1 h3,.calendar_name2 h3{font-size:16px; color:#070707; line-height:16px; font-weight:500;}
	.calendar_name1 p,.calendar_name2 p{font-size:12px; line-height:14px; padding-left:0;}

}