/*for render.js*/
.cover{
	background: none repeat scroll 0 0 #000000;
	position: absolute;
	left: 0;
	top: 0;
	opacity: 0.6;
	filter:alpha(opacity=60);
	width: 100%;
	z-index: 1999;
	height:110%;
}
.loading{
	height:100px;
	width:100px;
	position:absolute;
}
.fixed-tip{
	height:18px;
	padding-left:5px;
	padding-right:5px;
	background-color:#C6322A;
	color:white;
	position:fixed;
	top:1px;
	left:240px;
}
.popup-menu{
	position:fixed;
	width:240px;
	border:1px solid #CCCCCC;
	background:#FFFFFF;
	z-index:1999;
}
.popup-menu a,.popup-menu a:hover{
	display:block;
	width:228px;
	height:24px;
	padding-top:3px;
	padding-bottom:3px;
	padding-left:12px;
	float:left;
	text-decoration:none;
	color:#666666;
	font-size:14px;
	font-weight: normal;
	text-align: left;
}
.popup-menu a:hover{
	background:#CCCCCC;
}