html{
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
body{padding:0; margin:0;}
div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, textarea, blockquote, p { margin: 0; padding: 0; }
ul { list-style: none; }
li { list-style-type: none; }
img { border: 0; vertical-align: bottom; }
select, input { font-size: 12px; -webkit-appearance: none; }
address, cite, code, em, th { font-style: normal; font-weight: normal; }
.e_clear:after { clear: both; content: "."; display: block; height: 0; visibility: hidden; }
.e_clear{ zoom:1; }
textarea { background: #fff; border: 1px solid #d2d9dc; border-radius: 2px; box-shadow: 0 0 0 #d2d9dc; -moz-transition: .3s ease-in-out; transition-property: border box-shadow; -webkit-transition: .3s ease-in-out; }
textarea:focus, input[type="text"]:focus,input[type="password"]:focus,input[type="tel"]:focus { /*border: 1px solid #b7d4ea;*/ box-shadow: 0 0 5px #b7d4ea; }
a { color: #208de9; cursor: pointer; text-decoration: none; }
.none{display:none;}

.main{width:100%; padding-bottom:20px;}
.move_box,.snag,.nsnag{padding:20px 30px 0;}
.move_box h3,.snag h3,.nsnag h3{font-size:20px; font-family:"Hiragino Sans GB", "Microsoft YaHei", 微软雅黑, SimSun, arial; color:#000; text-align:center; line-height:40px;}
.mobile_box input{float:left; width:-webkit-calc(100% - 125px); height:22px; padding:5px; border:1px solid #ccc; font-size:14px; outline:none;}
.btn{float:right; width:100px; height:32px; line-height:30px; text-align:center; color:#fff; border:1px solid #4b9bc8; background:#58b6eb; margin-left:10px; font-size:14px;}
.code_box{padding:10px 0 15px 0;}
.code_box input{width:-webkit-calc(100% - 12px); height:22px; padding:5px; border:1px solid #ccc; font-size:14px; outline:none;}
.move_box p{color:#999; font-size:14px; line-height:180%;}
.grabbing_ticket{display:block;height:45px; width:-webkit-calc(100% - 2px); border:1px solid #cc6f0c;
	background:#f0830e; font-size:20px; color:#fff; line-height:45px; text-align:center;
	margin-top:15px;
}
.ticket_num{padding:10px 0; background:#ffcccc; margin:10px 0;}
.ticket_num p{font-size:15px; line-height:180%; text-indent:22px;}
.ticket_num p span{color:#000;}
.repeat_send{color:#999; font-size:12px; line-height:180%;}
.nsnag_bg p{ color:#000; font-size:14px; text-align:center; font-weight:600;  line-height:150%; text-align:left;}
.nsnag_bg{padding:30px 0 30px 70px; background:url(../images/nsnag.png) no-repeat 10px 22px; background-size:55px 58px;}
.goclock{background:#999; border-color:#999;}
.goclock:active,.goclock:hover{background:#999;}
.rules{padding-top:10px;}
.rules h3{font-size:14px; text-align:left;}
.rules p{line-height:160%;}


#js-phoneTip{
	display: none;
}

.imgTops,.imgTops img{width:100%;}