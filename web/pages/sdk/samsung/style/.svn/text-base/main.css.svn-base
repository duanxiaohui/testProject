.search_input{margin:0 9px 0 50px; position:relative; border:1px solid #c8c8c8; height:34px; box-shadow: 0px .5px 1px #c8c8c8; border-radius:3px; margin-top:6px; text-indent:0; line-height:48px;}
.clear_input{position:absolute; width:24px; height:24px; right:2px; top:5px; background:#f7f7f7 url(../images/del.jpg); background-size:100% 100%; z-index:9;}
