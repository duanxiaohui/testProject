.recommended_three li:nth-child(2){margin:0 5%;}
.recommended_calendar_list{padding:0 0 0 0;}
.recommended_calendar_list dl.on{background:#e0e0e0;}
.recommended_calendar_list dl{ padding:10px; border-bottom:1px solid #e0e0e0;}
.recommended_calendar_list dt{float:left; width:80px; height:50px; overflow:hidden;margin-right: 10px;}
.recommended_calendar_h3 {margin:8px 65px 0 0;}

/*无图片时*/
.recommended_calendar_list dl.noimg dt{display:none;}
.recommended_calendar_list dl.noimg dd{margin-left:0;}


.div_list{position:relative;}
.focus_btn{position:absolute; top:17px; right:10px; width:58px; height:35px; line-height:; text-align:center; border:1px solid #afafaf; border-radius:13px; -webkit-border-radius:13px; color:#6bda8d; }
.focus_btn div{display:inline-block; width:1px; height:35px; vertical-align: middle;}
.focus_btn b{font-weight:500; vertical-align: middle;}

.focus_btn:active{background:#e0e0e0;}
.div_list .on{color:#3d3d3d;}
.div_list .on div{display:inline-block; width:1px; height:35px; vertical-align: middle;}
.div_list .on b{font-weight:500; vertical-align: middle;}

.recommended_calendar_list dd h3{font-size:14px; margin-top:8px; color:#000000; font-weight:500; overflow: hidden;white-space:nowrap; text-overflow: ellipsis;}
.recommended_calendar_list dd p{color:#838383; overflow:hidden; white-space:nowrap; text-overflow: ellipsis;}
.schedule_content .footer_copy,.schedule_details_conetnt .footer_copy{border-top: solid 1px #e0e0e0;}

.footer_copy{height:16px; background:#fff url(../images/365@2x.png) no-repeat center center; width:100%; padding:26px 0; text-align:center; background-size:140px 16px;}

.classification_list{background:#fff; padding-top:34px;}
.classification_list dl{padding:10px 5%; border-bottom:1px solid #d4dbe0;}
.classification_list dl a{display:block;}
.classification_list dt{width:58px; float:left;}
.classification_list dd{margin-left:58px; line-height:40px; font-size:14px; color:#858585;}
.list_icon{width:40px; height:40px; border-radius:16px;}
.classification_list dl .list_icon{background:#ffa93a url(../images/sports_icon.png) no-repeat center center; background-size:44px 44px;}

.classification_list dd .arrow{float:right; width:10px; height:10px; border-top:1px solid #898e91; border-right:1px solid #898e91; transform:rotate(45deg);-webkit-transform:rotate(45deg); margin-top:15px; margin-right:5px;}
