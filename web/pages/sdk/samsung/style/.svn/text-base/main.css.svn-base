body { background: #fff; color: #444; font: 12px / 1.5em "Hiragino Sans GB", "Microsoft YaHei", 微软雅黑,SimSun,arial; margin: 0; padding: 48px 0 0 0;}
div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, textarea, blockquote, p { margin: 0; padding: 0; }
ul { list-style: none; }
li { list-style-type: none; }
img { border: 0; vertical-align: top; }
select, input { font-size: 12px; }
address, cite, code, em, th { font-style: normal; font-weight: normal; }
*{-webkit-tap-highlight-color: rgba(0,0,0,0); tap-highlight-color: rgba(0,0,0,0);}

a{text-decoration: none; }
html, body {
-webkit-user-select: none; /* 禁止选中文本（如无文本选中需求，此为必选项） */
user-select: none;
}
.none{display:none;}
.e_clear:after { clear: both; content: "."; display: block; height: 0; visibility: hidden; }
.e_clear { display: inline-block;}
.e_clear { display: block; }
.ce0{background:#e0e0e0;}

.bar{width:100%;height:48px; line-height:52px; font-size:20px; color:#000; background:#f7f7f7; text-align:left; position:fixed; top:0; z-index:999; text-indent:50px; overflow:hidden;}
.search_btn{position:absolute; width:35px; height:19px; top:19px; right:13px; line-height:19px; text-indent:0; color:#18a900; /*background:url(../images/search_icon.png) no-repeat; background-size:19px 19px; */font-size:14px; text-align:right;}
.search_btn:active{/*background:url(../images/search_icon_active.png) no-repeat; background-size:100% 100%;*/ color:#18a900;}
.nav{height:34px;background:#f7f7f7; text-align:center; position:fixed; top:48px; width:100%; z-index:999;}
.nav li{float:left; width:50%; height:32px; line-height:32px; border-bottom:2px solid transparent; color:#000;}
.nav li a{color:#000; display:block; width:100%; height:32px; line-height:32px; text-align:center; font-size:14px;}
.nav li:active{background:rgba(0,0,0,.1); border-radius: 10px}
.nav li.on{border-bottom:2px solid #18a900; }
.nav li.on a{color:#18a900;}
