*{-webkit-tap-highlight-color: rgba(0,0,0,0); tap-highlight-color: rgba(0,0,0,0);}
body { background: #fff; color: #444; font: 14px "Hiragino Sans GB", "Microsoft YaHei", 微软雅黑,SimSun,arial; height: 100%; margin: 0; padding: 0; }
div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, textarea, blockquote, p { margin: 0; padding: 0; }
ul { list-style: none; }
li { list-style-type: none; }
img { border: 0; vertical-align: top; }
select, input { font-size: 12px; }
address, cite, code, em, th { font-style: normal; font-weight: normal; }
table { border-collapse: collapse; border-spacing: 0; table-layout: fixed; }
.e_clear:after { clear: both; content: "."; display: block; height: 0; visibility: hidden; }
.e_clear { display: inline-block; }
.e_clear { display: block; }
textarea { background: #fff; border: 1px solid #d2d9dc; border-radius: 2px; box-shadow: 0 0 0 #d2d9dc; -moz-transition: .3s ease-in-out; transition-property: border box-shadow; -webkit-transition: .3s ease-in-out; }
textarea:focus, input[type="text"]:focus { /*border: 1px solid #b7d4ea;*/ box-shadow: 0 0 5px #b7d4ea inset; }
a { color: #208de9; cursor: pointer; text-decoration: none; }

section[data-id="1"] {
		background:#1a8bbd url(/pages/group_recommend/dist/img/first_bg1.png) no-repeat left bottom;
		background-size: 100% auto;
}
section[data-id="2"] {
	background:#1a8bbd url(/pages/group_recommend/dist/img/first_bg.png) no-repeat left bottom;
		background-size: 100% auto;
}
section[data-id="3"] {
	background:#1a8bbd url(/pages/group_recommend/dist/img/first_bg.png) no-repeat left bottom;
		background-size: 100% auto;
}

.first_top{min-height:106px;}
.first_info,.first_privilege{padding:0 8%;}
.pages h3{color:#ffe599; font-size:14px; padding:9px 0;}
.first_info p{color:#fff; font-size:12px; line-height:150%;}
.first_privilege li{width:78px; height:70px; background:#2d8cb5; border-radius: 3px; -webkit-border-radius:3px; float:left; margin:0 0 10px 5px; position:relative;}
.icon{padding:5px 0; text-align:center;}
.first_privilege li h4{font-size:14px; width:100%; color:#fff; transform:scale(.7); -webkit-transform:scale(.7); white-space:nowrap; position:absolute; bottom:10px; left:50%; text-align:center;}
.first_privilege li p{font-size:12px; color:#fff; transform:scale(.7); -webkit-transform:scale(.7); white-space:nowrap;
 position:absolute; bottom:0px; left:50%;}

 .second_rule{padding:5px 5% 0;}
 .second_rule li{font-size:12px; color:#fff; white-space:nowrap;transform:scale(.8); -webkit-transform:scale(.8); position:relative; line-height:24px;}
  .second_rule li:nth-of-type(1):after{
  	content:"1";
  	position:absolute;
  	width:15px;
  	height:15px;
  	text-align:center;
  	line-height:17px;
  	left:-28px;
  	border:2px solid #9ac5d6;
  	background:#0777af;
  	border-radius: 15px;
  	-webkit-border-radius: 15px;
  	overflow: hidden;
  	top:2px;
  }
    .second_rule li:nth-of-type(2):after{
  	content:"2";
  	position:absolute;
  	width:15px;
  	height:15px;
  	text-align:center;
  	line-height:17px;
  	left:-28px;
  	border:2px solid #9ac5d6;
  	background:#0777af;
  	border-radius: 15px;
  	-webkit-border-radius: 15px;
  	overflow: hidden;
  	top:2px;
  }
    .second_rule li:nth-of-type(3):after{
  	content:"3";
  	position:absolute;
  	width:15px;
  	height:15px;
  	text-align:center;
  	line-height:17px;
  	left:-28px;
  	border:2px solid #9ac5d6;
  	background:#0777af;
  	border-radius: 15px;
  	-webkit-border-radius: 15px;
  	overflow: hidden;
  	top:2px;
  }

  .view_btn{
  	float:right;
  	width:84px;
  	height:26px;
  	background:#0877ad;
  	border:2px solid #7db3ca;
  	border-radius: 3px;
  	-webkit-border-radius: 3px;
  	font-size:10px;
  	text-align:center;
  	line-height:26px;
  	margin-top:8px;
    color:#fff;
  }

  .second_process{padding:0px 5% 0;}
  .second_process li{width:42px; height:42px; border:3px solid #88bfd6; line-height:42px; text-align:center; color:#00a3d1; border-radius:30px; -webkit-border-radius:30px; float:left; font-size:12px; position:relative;}
  .second_process li:before{
    content: ' ';
    background-color:#e0eef4;
    width: 42px;
    height: 42px;
    border-radius: 30px;
    overflow: hidden;
    position: absolute;
    z-index: -1;
    top: 0;
    left: 0;
  }
  .second_process li.on{color:#fff; background:#248dc4;}
  .second_process li:last-child{
  	height:37px;
  	padding-top:5px;
  	line-height:16px;
  }
.second_process li:after{
	content:" ";
	width:5px;
	height:15px;
	position:absolute;
	background:url(/pages/group_recommend/dist/img/arrow_1.png) no-repeat;
	background-size:100% 100%;
	left:50%;
	top:13px;
	margin-left:37px;
}
 .second_process li:last-child:after{
 	display:none;
 }

  .second_process_div div{display:none; padding:5px 10px; font-size:12px; line-height:150%; color:#fff;}
  .second_process_div{min-height:64px; width:100%; background:#007099; margin-top:10px; border-radius: 3px; -webkit-border-radius: 3px;}

  .second_sign_btn{display:block;height:35px; width:90%; margin:0 5%; line-height:35px; background:#00a3d9; text-align:center; color:#fff; margin-top:15px; border:1px solid #fff; border-radius: 2px; -webkit-border-radius: 2px;}

  .third_list{padding:10px 0 0;}
  .third_list dl{padding:5px 5%;}
  .third_list dt{float:left; width:40px; height:40px; }
   .third_list dt img{border-radius: 20px; -webkit-border-radius: 20px;}
  .third_list dd{margin-left:50px;}
  .third_list dd h4{font-size:14px; color:#fff;}
  .third_list dd p{font-size:12px; color:#c3dce7;}

  .first_sign_btn{width:177px; height:42px; display:block; background:url(/pages/group_recommend/dist/img/first_btn.png); background-size:100% 100%; margin:15px 0 0 8%;}
  .third_list h3{padding:9px 5%;}
  .third__sign_btn{margin:15px 5% 5px; display:block; height:35px; line-height:35px; color:#fff; text-align:center; background:#00a3d9; border-radius: 3px; -webkit-border-radius: 3px; border:1px solid #fff;}
  .copy{font-size:12px; color:#fff; line-height:150%; text-align:center;}
.c00{background:#35b7f0;}


@media (device-height:480px) and (-webkit-min-device-pixel-ratio:2){/* 兼容iphone4/4s */
	.pages h3{padding:3px 0;}
  .third_list h3{padding:3px 5%;}
	.first_privilege li{margin-bottom:3px; height:50px;}
	.first_sign_btn{margin:5px auto 0;}
	.third_list dl:nth-of-type(4){display:none;}
	.third_list dl:nth-of-type(5){display:none;}
	.first_privilege li p{display:none;}
	.first_privilege li h4{bottom:-2px;}
	.first_top{margin-top:-10px;}
}
@media (device-height:667px) and (-webkit-min-device-pixel-ratio:2){/* 兼容iphone6 */

    .pages h3{padding:12px 0;}
    .third_list h3{padding:12px 5%;}
    .icon img{width:38px; height:38px;}
    .first_privilege li h4{
      bottom:6px;
    }
    .first_privilege li p{
      bottom:-2px;
    }

    .second_rule li{
      transform:scale(1); -webkit-transform:scale(1);
      padding-left:28px;
      margin:10px 0;
    }
    .second_rule li:nth-of-type(1):after{
      left:0;
    }
    .second_rule li:nth-of-type(2):after{
      left:0;
    }
    .second_rule li:nth-of-type(3):after{
      left:0;
    }

    .second_process_div{margin-top:20px;}
    .second_process_div div{padding:15px;}
    .second_process_div div p{line-height:180%;}
    .second_sign_btn{margin-top:25px;}
    .third_list dl {
      padding:15px 5%;
    }
    .copy{margin-top:60px; color:#00a3d9;}
}

@media (device-height:736px) and (-webkit-min-device-pixel-ratio:2){/* 兼容iphone6 Plus */
     .pages h3{padding:12px 0;}
     .third_list h3{padding:12px 5%;}
    .icon img{width:38px; height:38px;}
    .first_privilege li{width:110px; height:100px; margin-bottom:20px;}
    .first_privilege li h4{
      font-size: 13px;
      transform: scale(1); 
      -webkit-transform: scale(1);
      bottom:24px;
      left:0;
    }
    .first_privilege li p{
      font-size: 12px;
      width:100%;
      transform: scale(1); 
      -webkit-transform: scale(1);
      bottom:4px;
      left:0;
      text-align:center;
    }
    .first_sign_btn{margin-top:0;}

    .second_rule li{
      transform:scale(1); -webkit-transform:scale(1);
      padding-left:28px;
      margin:20px 0;
      font-size:14px;
    }
    .second_rule li:nth-of-type(1):after{
      width:20px;
      height:20px;
      line-height:20px;
      left:0;
    }
    .second_rule li:nth-of-type(2):after{
      width:20px;
      height:20px;
      line-height:20px;
      left:0;
    }
    .second_rule li:nth-of-type(3):after{
      width:20px;
      height:20px;
      line-height:20px;
      left:0;
    }

    .second_process_div{margin-top:20px;}
    .second_process_div div{padding:15px;}
    .second_process_div div p{line-height:180%;}
    .second_sign_btn{margin-top:25px;}
    .pages h3{font-size:18px;}
    .third_list dl {
      padding:15px 5%;
    }
    .third_list dd h4{font-size:18px;}
    .third_list dd p{font-size:14px;}
    .copy{margin-top:60px; color:#00a3d9;}
  }
@media (device-height:568px) and (-webkit-min-device-pixel-ratio:2){/* 兼容iphone5 */
    .pages h3{padding:8px 0;}
    .third_list h3{padding:8px 5%;}
    .icon img{width:38px; height:38px;}
    .first_privilege li{margin-bottom:5px;}
    .first_privilege li h4{
      bottom:6px;
    }
    .first_privilege li p{
      bottom:-2px;
    }
    .first_sign_btn{margin-top:5px;}

    .second_rule li{
      transform:scale(1); -webkit-transform:scale(1);
      padding-left:28px;
      margin:5px 0;
    }
    .second_rule li:nth-of-type(1):after{
      left:0;
    }
    .second_rule li:nth-of-type(2):after{
      left:0;
    }
    .second_rule li:nth-of-type(3):after{
      left:0;
    }

    .second_process_div{margin-top:20px;}
    .second_process_div div{padding:15px;}
    .second_process_div div p{line-height:180%;}
    .second_sign_btn{margin-top:20px;}
    .third_list dl {
      padding: 9px 5%;
    }
    .copy{margin-top:20px; color:#00a3d9;}
}