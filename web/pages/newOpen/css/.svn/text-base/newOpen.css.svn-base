.create_upload_imglist li{width:48px; height:48px; overflow:hidden; float:left; margin-right:10px; border:1px solid #e2e2e2; position:relative; text-align:center;}
.create_upload_imglist li img{height:100%; margin:0 auto;}
.create_upload_imglist li:hover .del_img_btn{display:block;}
.del_img_btn{width:15px; height:15px; position:absolute; z-index:2; top:-1px; right:-1px; display:none; background:#f00; cursor:pointer; border:1px solid #ccc;}
.del_img_btn:after{content:" ";width:2px; height:12px; background:#fff; position:absolute;
left:6px; top:1px;
	-webkit-transform: rotate(45deg);
    -moz-transform: rotate(45deg);
    -o-transform: rotate(45deg);
 }
 .del_img_btn:before{content:" ";width:2px; height:12px; background:#fff; position:absolute;
left:6px; top:1px;
	-webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
 }
.upload_image_link{float:left;width:48px; height:48px; border:1px solid #e2e2e2; background:#e2e2e2;position:relative;}
.upload_image_link:after{content:""; position:absolute; width:4px; height:30px; left:50%; top:50%; margin:-15px 0 0 -2px; background:#fff;
	}
.upload_image_link:before{content:""; position:absolute; width:30px; height:4px; left:50%; top:50%; margin:-2px 0 0 -15px; background:#fff;}

input[name="linked_url"],input[name="location"],input[name="schTitle"]{height:20px; border:1px solid #c4c4c4; width:332px; outline:none; padding:4px 5px;}
input[name="location"]{float:left; width:303px; border-right:0;}
.js_map_address{float:left; width:30px; height:30px; background:url(../image/address.jpg);}


/*login*/
.login_reg_div{width:410px; margin:0 auto; padding-top:75px;}
.login_reg_div h2{height:36px; line-height:36px; padding-left:54px; background:url(../image/login_icon.jpg) no-repeat left center; font-size:24px; color:#575757; font-weight:500; margin-bottom:25px;}
.reg_in_btn{float:right; margin-right:5px; padding-right:30px; background:url(../image/reg_icon.jpg) no-repeat right center; height:36px; line-height:36px; color:#6dd768;}
.login_reg_div dl{padding:9px 0;}
.login_reg_div dt{width:64px; padding-right:10px; height:38px; line-height:38px; text-align:right; font-size:14px; float:left;}
.login_reg_div dd{ margin-left:74px; height:38px; position:relative;}
.login_reg_div dd input{height:22px; border:1px solid #c4c4c4; width:324px; padding:7px 5px; outline:none;}
.code_img{width:80px; height:20px; padding:8px 0; float:left; text-align:center;}
.login_reg_div dd.code_div input{float:left; width:244px;}

.login_cookie a{float:right; margin-top:5px;}
.login_cookie label{float:left;}
.login_reg_div dd.login_cookie input{width:20px; height:20px; margin-top:8px; vertical-align: middle}
.login_reg_div dd.login_cookie span{vertical-align: middle; display:inline-block; margin-top:5px;}
.login_btn_div{width:336px; margin:0 auto; padding:5px 0 0 74px;}
.login_btn{display:block; width:336px; height:60px; line-height:60px; text-align:center; color:#fff; font-size:24px; background:#3ab7ff;}
.login_btn:hover{color:#fff; background:#00a2ff;}
.login_error_tips{padding-left:30px; position:absolute; left:0px; top:-30px; height:30px; line-height:30px; color:#f00;}

