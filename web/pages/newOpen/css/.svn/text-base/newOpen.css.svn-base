.add_btn, .lessen_btn{
	position: absolute;
	background-color: #fff;
	border-radius: 100% 100% 100% 100%;
	width: 19px;
	height: 19px;
	top: 0;
	bottom: 0;
	margin: auto;
}
.add_btn{
	left: -10px;
}
.add_btn:after{
	content: '';
	position: absolute;
	border-bottom: 1px solid #0093d8;
}
.lessen_btn{
	right: -10px;
}
.lessen_btn:before{
	content: '';
	position: absolute;
	border-left: 1px solid #0093d8;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	margin: auto;
	height: 9px;
	width: 0;
}
.lessen_btn:after{
	content: '';
	position: absolute;
	border-bottom: 1px solid #0093d8;
}
.add_btn:after, .lessen_btn:after{
	width: 9px;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	margin: auto;
	height: 0;
}
