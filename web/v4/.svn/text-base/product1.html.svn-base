        if ($(window).scrollTop() > 600) {
            $(".on").removeClass("on");
			$("#business").addClass("on");
        }
        if ($(window).scrollTop() > 1150) {
			$(".on").removeClass("on");
            $("#todo").addClass("on");
        }
		if ($(window).scrollTop() > 1800) {
			$(".on").removeClass("on");
            $("#wnl").addClass("on");
        }
		if ($(window).scrollTop() > 2550) {
