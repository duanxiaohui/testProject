.tab_button {
	height: 30px;
	width: 90px;
	margin-top: 4px;
	border-radius: 4px 4px 0px 0px;
	-webkit-box-shadow: 0px 2px 2px #ffffff inset;
	float: right;
	background: -webkit-gradient(linear, 0 0, 0 100%, from(#e9d2b2), to(#f2dec3));
	border: 1px solid #bdaa80;
	color: #603703;
	line-height: 30px;
	font-size: 15px;
	text-align: center;
	border-bottom: 0px;
}
.active_tab_button {
	background: -webkit-gradient(linear, 0 0, 0 100%, from(#f5f1e8), to(#ffffff));
}
#jieri_button {
	margin-right: 10px;
}
#nongli_button {
	margin-right: 0px;
	border-right: 0px;
}
#mainNongli, #mainJieri {
	color: #816526;
	border-radius: 5px;
	background-color: white;
	padding-bottom: 20px;
}
#mainJieri {
	display: none;
}
#nongliTitle, #jieriTitle {
	font-size: 20px; 
	height: 40px;
