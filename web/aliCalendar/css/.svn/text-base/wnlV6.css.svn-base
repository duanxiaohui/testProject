}
#jieriTable {
	border-collapse:collapse;
	width: 100%;
	text-align: center;
}
.jieriTd {
	width: 50%;
	height: 60px;
	border: 1px solid #efe6d5;
}
.jieriName {
	font-size: 16px;
	text-align: center;
}
.jieriDate {
	font-size: 10px;
	text-align: center;
}
#mainJieqi {
	border-radius: 10px;
	background-color: white;
	padding-bottom: 20px;
}
#jieqiName {
