	.cds_scrollbar {
		position: absolute;
		background: #f9f9f9;  border:1px solid #ddd ; 
		right: 0px; 
		cursor: pointer;
	}
	.cds_scrollbar_over {
		background: #f5f5f5; 
	}
	.cds_scrollbar_pushed {
		background: #f9f9f9 ; 
	}
	.cds_track {
		position: absolute;
		background: #f9f9f9 ;
		right: -1px;
		top:0px;
		height: 99%;
		cursor: pointer;
	}
	.cds_up {
		position: absolute;
		background: #f9f9f9 ; border:1px solid #ddd;
		right: 0px; top:0px; 
		
		cursor: pointer;
	}
	.cds_down {
		position: absolute;
		background: #f9f9f9; border:1px solid #ddd;
		right: 0px;
		bottom: 0px;
		cursor: pointer;
	}



