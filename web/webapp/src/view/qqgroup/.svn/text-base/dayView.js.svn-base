/*
**	DayView
*/
define(function(require, exports, module) {
	module.exports = DayView;
	
	//local variable
	var containerHtml = "<div style='text-align:center;'>日视图正在开发ing</div>";
	
	function DayView(container, loadSchedule){
		var self = this;
		self.container = container;
		self.loadSchedule = loadSchedule;
		self.container.html(containerHtml);
		//event
		self.initEvent();
	}
	
	DayView.prototype.initEvent = function(){
		var self = this;
		
	}
	
	DayView.prototype.show = function(){
		this.container.show();
	}
	DayView.prototype.hide = function(){
		this.container.hide();
	}
	
});
